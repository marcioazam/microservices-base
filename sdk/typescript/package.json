{
  "name": "@auth-platform/sdk",
  "version": "0.1.0",
  "description": "Auth Platform TypeScript SDK",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "auth",
    "authentication",
    "oauth",
    "passkeys",
    "webauthn",
    "caep"
  ],
  "author": "Auth Platform Team",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "fast-check": "^3.15.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "jose": "^5.0.0"
  },
  "peerDependencies": {
    "@simplewebauthn/browser": "^9.0.0"
  },
  "peerDependenciesMeta": {
    "@simplewebauthn/browser": {
      "optional": true
    }
  }
}
